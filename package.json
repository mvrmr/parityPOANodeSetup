{
  "name": "mcc_poc",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run backend",
    "cleanDbNstart": "npm-run-all --silent --parallel truffle-migrate backend",
    "everything": "npm-run-all --silent --parallel backend frontend lint:watch",
    "backend": "./node_modules/.bin/nodemon --watch src --ext js,pug --exec babel-node -- ./src/tools/backend.js",
    "frontend": "babel-node -- ./src/tools/frontend.js",
    "lint:watch": "npm run lint -- --watch",
    "prebuild": "npm run clean-dist && npm run lint",
    "build": "babel-node src/tools/build.js",
    "mongoDB-dump": "mongodump --db mccDB --out data/mongoDBdump/",
    "mongoDB-restore": "mongorestore --drop -d mccDB 'data/mongoDBdump/mccDB'",
    "mongoDB-restore-docker": "docker exec mcc-mongo-DB mongorestore --drop -d mccDB /mcc_poc/data/mongoDBdump/mccDB",
    "truffle-migrate": "truffle migrate --reset",
    "test-rpc": "testrpc -m \"cat category sport grid anger game language leisure edge dumb canvas feel\"",
    "parity": "parity --jsonrpc-cors 'http://ethereum.github.io/browser-solidity' -jw --jsonrpc-interface 127.0.0.1 --jsonrpc-port 8545 --identity 'Ethereum Parity Node on' --nodiscover --nat none --chain dev --jsonrpc-apis 'web3,eth,net,parity,rpc,parity_accounts,personal,traces,rpc' --author 0x00296DeBe65f9F0f4c0D269939e061e4515594e4 --unlock 0x00296DeBe65f9F0f4c0D269939e061e4515594e4 --password passwords/parity.pass --ipcpath './geth.ipc' --force-ui --reseal-min-period 0 --gas-floor-target 4300000 --gas-cap 20000000000"
  },
  "engines": {
    "node": "~4.2.1",
    "npm": "~2.2.0"
  },
  "dependencies": {
    "babel-cli": "^6.14.0",
    "body-parser": "^1.19.0",
    "consolidate": "^0.14.5",
    "cookie-parser": "~1.4.3",
    "crypto2": "^0.3.1",
    "debug": "~2.2.0",
    "ejs": "^2.5.6",
    "eslint-plugin-import": "^1.16.0",
    "express": "^4.17.1",
    "express-session": "^1.15.2",
    "jquery": "^3.1.1",
    "js-sha256": "^0.3.2",
    "lodash": "^4.16.6",
    "moment": "^2.17.0",
    "mongoose": "~4.10",
    "morgan": "^1.10.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "3.0.0",
    "request": "^2.75.0",
    "truffle-contract": "^1.1.10",
    "web3": "^0.17.0-beta"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-core": "6.14.0",
    "babel-loader": "6.2.5",
    "babel-plugin-remove-comments": "^2.0.0",
    "babel-plugin-transform-es2015-for-of": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-babili": "0.0.8",
    "babel-preset-env": "^1.5.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "6.14.0",
    "browser-sync": "^2.26.13",
    "bs-fullscreen-message": "^1.1.0",
    "clean-css": "^3.4.20",
    "coveralls": "2.11.12",
    "cross-env": "2.0.1",
    "css-loader": "0.24.0",
    "enzyme": "2.4.1",
    "eslint": "3.9.1",
    "eslint-watch": "2.1.14",
    "extract-text-webpack-plugin": "1.0.1",
    "fastclick": "^1.0.6",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "img-loader": "^1.3.1",
    "json-loader": "^0.5.4",
    "material-design-lite": "^1.2.1",
    "mockdate": "1.0.4",
    "node-sass": "^3.10.1",
    "open": "6.0.0",
    "postcss-loader": "0.11.0",
    "prompt": "1.0.0",
    "replace": "0.3.0",
    "resolve-url-loader": "^1.6.0",
    "sass-loader": "4.0.0",
    "sass-resources-loader": "1.1.0",
    "strip-ansi": "^3.0.1",
    "style-ext-html-webpack-plugin": "^2.0.5",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7"
  }
}
